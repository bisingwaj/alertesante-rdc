
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// NOTE: Si Prisma 7 force le retrait de 'url', il faut utiliser prisma.config.ts.
// MAIS pour l'instant, je tente de downgrader Prisma en v5 pour la stabilité et éviter de refaire toute la config maintenant.
